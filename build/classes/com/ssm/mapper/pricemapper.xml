<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssm.mapper.Pricemapper">
        <select id="findbaseinformationById" parameterType="int" resultType="com.ssm.entity.Baseinfomations">
             select * from baseinfomations where id=#{id}
            
        </select>
        <select id="findfunctionById" parameterType="int" resultType="com.ssm.entity.Function">
           select * from function where funid=#{id}
        </select>
        <select id="findFacadeinfomationById" parameterType="int" resultType="com.ssm.entity.FacadeInfomations">
             select * from facadeinfomations where id=#{id}
        </select>
</mapper>
